% stating of prefix for YA - Year End cards
(YA)
{
		/VAR_YA		   PREFIX	SETVAR	% prefix for year end cards
		/VAR_YCT	   FLD1		SETVAR	% card type
		/VAR_YCN	   FLD2		SETVAR
		/VAR_YCS	   FLD3		SETVAR
		
		/VAR.Y4	 00	ADD
		/VAR.Y5	 00	ADD

	
		IF VAR_YA (YA) eq
		{	
			(CASIOS.FRM) SETFORM
	
			/VAR_COUNTYA ++ 
			IF VAR_COUNTYA 1 eq
			{
			/VAR_ARRAY1F1 () SETVAR
			/VAR_ARRAY2F1 () SETVAR
			/VAR_ARRAY3F1 () SETVAR
			/VAR_ARRAY4F1 () SETVAR
			/VAR_ARRAY5F1 () SETVAR
			/VAR_ARRAY6F1 () SETVAR
			/VAR_ARRAY7F1 () SETVAR	
			}
			ENDIF
 		
		F7			
		%05 SETLSP

		%12 MOVEH VAR_YCT VSUB SH
		%100 MOVEH VAR_YCN VSUB SH
		%140 MOVEH VAR_YCS VSUB SH

		% Assign YA Variables Here
		 IF VAR_COUNTYA 1 eq	
		  {  	  
		  /VAR_ARRAY1F1 FLD1 SETVAR
		  }
		 ENDIF
		 IF VAR_COUNTYA 2 eq
		  {
		  /VAR_ARRAY2F1 FLD1 SETVAR
		  }
		 ENDIF
		 IF VAR_COUNTYA 3 eq
		  {
		  /VAR_ARRAY3F1 FLD1 SETVAR
		  }
		 ENDIF
		 IF VAR_COUNTYA 4 eq
		  {
		  /VAR_ARRAY4F1 FLD1 SETVAR
		  }
		 ENDIF
		 IF VAR_COUNTYA 5 eq
		  {
		  /VAR_ARRAY5F1 FLD1 SETVAR
		  }
		 ENDIF
		 IF VAR_COUNTYA 6 eq
		  {
		  /VAR_ARRAY6F1 FLD1 SETVAR
		  }
		 ENDIF
		 IF VAR_COUNTYA 7 eq
		  {
		  /VAR_ARRAY7F1 FLD1 SETVAR
		  }
		 ENDIF

	
  		} ENDIF

}
