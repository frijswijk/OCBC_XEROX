CASE PREFIX
{}
% ------------------------------------------------
(MR)	% starting of prefix for MR - Marker for MSM Printing
{
		% define page-break
		%  IF PREFIX (MR) eq				   % new document
	 %{ 
		PAGEBRK NEWFRONT
		[ [    08    25   190   255     0 ]] SETLKF
		/VAR_COUNTTD 0 SETVAR

		%Reset Variables For YA
		/VAR_COUNTYA 0 SETVAR

		% Reset Page Count 
 		/VAR_COUNTPAGE 0 SETVAR			% reset page numbering counter

		% Initiate SETPAGENUMBER at 1st record of customer
		IF CPCOUNT 1 eq					% new document
		{ /VAR_pctot 0 SETVAR }			% reset page count
		
			ELSE
			
			{ % modified on 270709 for transaction description
			  /VAR_COUNTTD 0 SETVAR 	% reset transaction description counter (34 lines)
			  /VARdoc ++
			   VARtab VARdoc GETITEM
			   
			 ( PAGE # OF $$VAR_pctot. ) VSUB 1 200.5 287 6 SETPAGENUMBER		
		
		} ENDIF
 %} ENDIF
